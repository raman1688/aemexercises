<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="community-community-minqg-members"
        rep:privileges="{Name}[jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="community-community-minqg-sitecontentmanager"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,jcr:readAccessControl,rep:write,jcr:lockManagement]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="social-enablement-replication-user"
        rep:privileges="{Name}[jcr:modifyAccessControl,jcr:read,jcr:readAccessControl]"/>
</jcr:root>
